                        <Image url="@../../../images/car2.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="Robot" fitHeight="17.0" fitWidth="16.0" layoutX="4.0" layoutY="169.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../images/robot.png" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
         </content>
      </Tab>
      <Tab text="Maze_2_tab">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="Maze_2" fitHeight="288.0" fitWidth="385.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../images/maze2.png" />
