                  <ImageView fx:id="Maze_1" fitHeight="281.0" fitWidth="501.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../images/maze.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="car_one" fitHeight="21.0" fitWidth="59.0" layoutX="20.0" layoutY="167.0" onKeyPressed="#user_move" pickOnBounds="true" preserveRatio="true">
